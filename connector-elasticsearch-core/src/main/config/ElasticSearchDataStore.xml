<?xml version="1.0" encoding="UTF-8"?>


<DataStore>
    <Name>ElasticSearch</Name>
    <Version>0.4.0</Version>
    <RequiredProperties>
        <Property>
            <PropertyName>Hosts</PropertyName>
            <Description>The list of hosts ips</Description>
        </Property>
        <Property>
            <PropertyName>Port</PropertyName>
            <Description>The list of hosts ports</Description>
        </Property>
    </RequiredProperties>
    <Behaviors>
        <Behavior>ALL_INDEXED</Behavior>
    </Behaviors>
</DataStore>



